{
  "name": "valetudo-frontend",
  "homepage": ".",
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "node scripts/build.js",
    "build_stats": "node scripts/build.js --stats",
    "analyze_stats": "npx webpack-bundle-analyzer ./build/bundle-stats.json",
    "watch": "node scripts/watch.js",
    "ts-check": "tsc --noEmit --skipLibCheck",
    "lint": "eslint -c .automated.eslintrc.json .",
    "lint_fix": "eslint -c .automated.eslintrc.json . --fix",
    "lint_pedantic": "eslint -c .pedantic.eslintrc.json .",
    "prepare_commit": "npm run lint_fix && npm run ts-check"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "11.13.3",
    "@emotion/styled": "11.13.0",
    "@fontsource/ibm-plex-sans": "5.2.8",
    "@fontsource/jetbrains-mono": "5.1.1",
    "@mui/base": "5.0.0-beta.58",
    "@mui/icons-material": "6.1.3",
    "@mui/lab": "6.0.0-beta.10",
    "@mui/material": "6.1.1",
    "@tanstack/react-query": "5.59.13",
    "@tanstack/react-query-devtools": "5.59.13",
    "axios": "1.6.2",
    "eliza-as-promised": "git+https://npm@github.com/Hypfer/eliza-as-promised#0.0.3",
    "notistack": "3.0.1",
    "react": "18.3.1",
    "react-div-100vh": "0.7.0",
    "react-dom": "18.3.1",
    "react-joystick-component": "6.2.1",
    "react-markdown": "9.0.1",
    "react-router-dom": "6.30.3",
    "reconnecting-eventsource": "1.6.2",
    "rehype-raw": "7.0.0",
    "remark-gfm": "4.0.0",
    "semaphore": "1.1.0",
    "use-long-press": "3.2.0",
    "uuid": "10.0.0",
    "zustand": "4.4.7"
  },
  "devDependencies": {
    "@babel/core": "7.28.6",
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@svgr/webpack": "8.1.0",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@types/uuid": "10.0.0",
    "babel-loader": "10.0.0",
    "babel-preset-react-app": "10.1.0",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "css-loader": "7.1.2",
    "css-minimizer-webpack-plugin": "7.0.4",
    "eslint-config-react-app": "7.0.1",
    "eslint-webpack-plugin": "3.2.0",
    "fs-extra": "9.0.1",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "2.10.0",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-loader": "8.2.0",
    "postcss-normalize": "13.0.1",
    "postcss-preset-env": "11.1.1",
    "react-dev-utils": "12.0.1",
    "resolve": "1.22.11",
    "style-loader": "4.0.0",
    "terser-webpack-plugin": "5.3.16",
    "webpack": "5.104.1",
    "webpack-manifest-plugin": "6.0.1"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
