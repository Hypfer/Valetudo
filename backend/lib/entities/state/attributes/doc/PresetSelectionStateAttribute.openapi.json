{
  "components": {
    "schemas": {
      "PresetSelectionStateAttribute": {
        "allOf": [
          {
            "$ref": "#/components/schemas/StateAttribute"
          },
          {
            "type": "object",
            "properties": {
              "type": {
                "type": "string",
                "enum": [
                  "fan_speed",
                  "water_grade",
                  "operation_mode"
                ]
              },
              "value": {
                "type": "string",
                "enum": [
                  "off",
                  "min",
                  "low",
                  "medium",
                  "high",
                  "max",
                  "turbo",
                  "custom",
                  
                  "vacuum",
                  "mop",
                  "vacuum_and_mop",
                  "vacuum_then_mop"
                ]
              },
              "customValue": {
                "type": "number"
              }
            }
          }
        ]
      }
    }
  }
}
