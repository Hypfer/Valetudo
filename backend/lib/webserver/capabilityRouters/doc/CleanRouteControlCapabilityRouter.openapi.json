{
  "/api/v2/robot/capabilities/CleanRouteControlCapability": {
    "get": {
      "tags": [
        "CleanRouteControlCapability"
      ],
      "summary": "Get current clean route",
      "responses": {
        "200": {
          "description": "Ok",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "interval": {
                    "type": "string",
                    "enum": [
                      "normal",
                      "quick",
                      "intensive",
                      "deep"
                    ]
                  }
                }
              }
            }
          }
        }
      }
    },
    "put": {
      "tags": [
        "CleanRouteControlCapability"
      ],
      "summary": "Set clean route",
      "requestBody": {
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "interval": {
                  "type": "string",
                  "enum": [
                    "normal",
                    "quick",
                    "intensive",
                    "deep"
                  ]
                }
              }
            }
          }
        }
      },
      "responses": {
        "200": {
          "$ref": "#/components/responses/200"
        },
        "400": {
          "$ref": "#/components/responses/400"
        }
      }
    }
  },
